{
  "buildCommand": "pnpm build",
  "outputDirectory": "dist",
  "git": {
    "deploymentEnabled": {
      "main": true,
      "dev": true
    }
  },
  "functions": {
    "api/index.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    }
  ]
}
