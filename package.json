{
  "name": "khqr-sdk",
  "version": "1.0.0",
  "author": "manethpak <manethpak.dev@gmail.com>",
  "description": "Typescript KHQR SDK for Bakong API payment",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./constants": {
      "import": {
        "types": "./dist/constants/index.d.mts",
        "default": "./dist/constants/index.mjs"
      },
      "require": {
        "types": "./dist/constants/index.d.ts",
        "default": "./dist/constants/index.js"
      }
    },
    "./helper": {
      "import": {
        "types": "./dist/helper/index.d.mts",
        "default": "./dist/helper/index.mjs"
      },
      "require": {
        "types": "./dist/helper/index.d.ts",
        "default": "./dist/helper/index.js"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/types/index.d.mts",
        "default": "./dist/types/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/types/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manethpak/khqr-sdk.git"
  },
  "scripts": {
    "build": "tsdown",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "keywords": [
    "khqr",
    "bakong",
    "sdk",
    "typescript"
  ],
  "license": "ISC",
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "@better-fetch/fetch": "^1.1.18",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "tsdown": "^0.15.6",
    "typescript": "^5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}